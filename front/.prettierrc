{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "overrides": [
    {
    "files": "*.svelte",
    "options": {
        "plugins": [ "prettier-plugin-svelte" ]
      }
    }
  ],
  "pluginSearchDirs": [ "." ]
}
